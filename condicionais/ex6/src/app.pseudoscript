
modelo Multiplo:
    construtor():
        Multiplo.numero = 0

    funcao start():
        entrada("Digite o número: ", Multiplo.numero)
        verificacaoMultiplo(Multiplo.numero)
        
    #funcao que verifica se o número introduzido é multiplo por 5.

    funcao verificacaoMultiplo(numero):
        se media % 5 == 0:
            imprime("O número é multiplo por 5.")
        senao:
            imprime("O número não é multiplo por 5.")
        